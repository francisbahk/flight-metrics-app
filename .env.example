# Amadeus API Credentials
# Get from: https://developers.amadeus.com
AMADEUS_API_KEY=your_api_key_here
AMADEUS_API_SECRET=your_api_secret_here

# Gemini API Key
# Get from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_key_here
# OR
GOOGLE_API_KEY=your_gemini_key_here

# MySQL Database Configuration
# For local development with MySQL running on localhost
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=flight_rankings
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password

# Note: Create the database first with:
# mysql -u root -p
# CREATE DATABASE flight_rankings;
# exit;
#
# Then run: python setup_database.py
